select count(*) as conteggio_Charles from customer where fname = &quot;Charles&quot;;
select count(*) as conteggio_Patricia from customer where fname = &quot;Patricia&quot;;
select count(*) as conteggio_Sharon from customer where fname = &quot;Sharon&quot;;
-- meglio nel seguente modo --
select fname, count(*) from customer
where fname=&quot;Charles&quot; or fname=&quot;Patricia&quot; or fname=&quot;Sharon&quot; group by fname;

select count(*) as conteggio, member_card.description
from customer join member_card on customer.card_type_id=member_card.card_type_id 
where description=&quot;Bronze&quot; or description=&quot;Silver&quot; or description=&quot;Gold&quot; 
or description=&quot;Normal&quot; group by description;

select count(*) as conteggio, customer.lname
from customer join bill on customer.customer_id=bill.customer_id 
where lname=&quot;Dixon&quot; or lname=&quot;Emerson&quot; group by lname;


select * from bill, customer 
where bill.customer_id=customer.customer_id and bill.total&gt;307;
select * from bill, customer 
where bill.customer_id=customer.customer_id and bill.total&gt;307 and lname=&quot;Doman&quot; and fname=&quot;Bradley&quot;;
-- punto 5 dell'esercizio 3 --

SELECT customer.lname, customer.fname FROM bill, customer, item_in_bill, product
WHERE customer.customer_id=bill.customer_id AND bill.bill_id=item_in_bill.bill_id AND
item_in_bill.product_id=product.product_id AND product.product_name=&quot;Great Muffins&quot; ORDER BY customer.lname;

-- punto 6 dell'esercizio 3 --
-- mio: --
SELECT customer.lname, customer.fname, product.product_name, bill.date FROM supplier, customer, bill, product
WHERE customer.customer_id=bill.customer_id and supplier.supplier_id=product.supplier_id
AND supplier.name=&quot;Bravo&quot;;
-- soluzione: --
SELECT customer.lname, customer.fname, product.product_name, bill.date FROM bill, customer, item_in_bill, product, supplier
WHERE customer.customer_id=bill.customer_id AND
bill.bill_id=item_in_bill.bill_id AND item_in_bill.product_id=product.product_id AND
product.supplier_id=supplier.supplier_id AND supplier.name=&quot;Bravo&quot;;

